<div class="container">
  <h1>Flagged Questions</h1>
  <table class="flagged-questions-table">
    <thead>
      <tr>
        <th>Question</th>
        <th>Answer</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% questions.forEach(question => { %>
      <tr>
        <td><%= question.question %></td>
        <td><%= question.answer %></td>

        <td>
          <a href="/modify-questions/<%= question._id %>/edit" class="edit-btn"
            >Edit</a
          >
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
